<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Title</th>
      <th scope="col">Descripcion</th>
      <th scope="col">Price</th>
      <th scope="col">Code</th>
      <th scope="col">Stock</th>
      <th scope="col">Category</th>
      <th scope="col">Thumbnail</th>
      <th scope="col">Status</th>
    </tr>
  </thead>
  <tbody>
    {{#each products}}
    <tr>
      <th scope="row">{{id}}</th>
      <td>{{title}}</td>
      <td>{{des}}</td>
      <td>{{price}}</td>
      <td>{{code}}</td>
      <td>{{stock}}</td>
      <td>{{category}}</td>
      <td>{{thumbnail}}</td>
      <td>{{status}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>